#include<stdio.h>
void bharat(){
    printf("you are in bharat\n");
    return;
}
void england(){
    printf("you are in england\n");
    bharat();
    return;
}
void australia(){
    printf("you are in australia\n");
    england();
    return;
}    
void india(){
    printf("you are in india\n");
    australia();
    return;
}
int main(){
    india();
    return 0;
}
