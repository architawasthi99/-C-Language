#include<stdio.h>
int main()
{
    int a,b,c;
    int max=0,min=0,mid=0;
    printf("enter the value of a,b,c");
    scanf("%d%d%d",&a,&b,&c);
    if(a>b)
    {
        if(a>c)
        {
            max=a;
            if(b>c)
            {
                mid=b;
                min=c;
            }
        }
        else
        {
            mid=c;
            min=b;
        }
    }
    else
    {
        max=c;
        mid=a;
        min=b;
    }
    else
    {
        if(b>c)
        {
            max=b;
            if(a>c)
            {
                mid=c;
                min=a;
            }
        }
        else
        {
            max=c;
            mid=b;
            min=a;
        }
    }
    printf("%d is maximum\n%d is middle\n%d is minimum",max,mid,min);
    return 0;
}
