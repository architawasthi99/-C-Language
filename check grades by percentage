#include<stdio.h>
int main ()
{
  int p;
  printf ("ENTER THE PERCENTAGE YOU SCORED : ");
  scanf ("%d", &p);
  if (p <= 100 && p >= 90)
	printf ("EXCELLENT STUDENT %d", p);
  else if (p <= 89 && p >= 80)
	printf ("VERY GOOD STUDENT %d", p);
  else if (p <= 79 && p >= 70)
	printf ("GOOD STUDENT %d", p);
  else if (p <= 69 && p >= 60)
	printf ("CAN DO BETTER %d", p);
  else if (p <= 59 && p >= 50)
	printf ("AVERAGE STUDENT %d", p);
  else
	printf ("STUDENT FAILED IN EXAMS %d", p);
  return 0;
}
