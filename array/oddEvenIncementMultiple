#include<stdio.h>
int main(){
    int arr[100],i,n;
    printf("enter total elements : ");
    scanf("%d",&n);
    for(i=0;i<n;i++){
        printf("enter the elementsm : %d ",i+1);
        scanf("%d",&arr[i]);
        if(i%2!=0){
            arr[i]=arr[i]*2;
        }
        else
        arr[i]=arr[i]+10;
    }
    for(i=0;i<n;i++)
    printf("%d ",arr[i]);
    return 0;
}
