#include<stdio.h>
#include<math.h>
int main(){
    int n,arr[100],i;
    printf("enter the array size : ");
    scanf("%d",&n);
    for (i=0;i<n;i++){
        printf("enter the elements %d---",i+1);
        scanf("%d",&arr[i]);
    }
    for(i=0;i<n;i++){
    if(arr[i]<35){
    printf("INDEX NUMBER IS %d",i);
    }
    }
    return 0;
}
