1234
1234

#include<stdio.h>
int main()
{
    int a,b;
    printf("enter the row number");
    scanf("%d",&a);
    printf("enter the column number");
    scanf("%d",&b);
    for(int i=1;i<=b;i++)
    {
        for(int j=1;j<=a;j++)
        {
            printf("%d",j);
        }
        printf("\n");
    }
    return 0;
}
